{
   "type":"object",
   "required":true,
   "additionalProperties":false,
   "properties":{
      "authors":{
         "type":"object",
         "required":false
      },
      "categories":{
         "type":"array",
         "required":false,
         "uniqueItems":true,
         "items":{
            "type":"string"
         }
      },
      "references":{
         "type":"object",
         "required":false,
         "patternProperties":{
            "/^.*$/":{
               "type":"string",
               "format":"uri"
            }
         }
      },
      "description":{
         "type":"string",
         "required":false
      },
      "iconUrl":{
         "type":"string",
         "required":false,
         "format":"uri"
      },
      "logoUrl":{
         "type":"string",
         "required":false,
         "format":"uri"
      },
      "uid":{
         "type":"string",
         "required":true,
         "minLength":3
      },
      "repo":{
         "type": "string",
         "required":true,
         "minLength":2
      },
      "modId":{
         "type":"string",
         "required":false
      },
      "name":{
         "type":"string",
         "required":true
      },
      "nemName":{
         "type":"string",
         "required":false
      },
      "tags":{
         "type":"array",
         "required":false,
         "items":{
            "type":"string"
         }
      },
      "updateUrl":{
         "type":"string",
         "required":true
      },
      "versions":{
         "type":"array",
         "required":true,
         "items":[
            {
               "type":"object",
               "required":false,
               "additionalProperties":false,
               "properties":{
                  "mcCompat":{
                     "type":"array",
                     "required":true,
                     "items":{
                        "type":"string"
                     },
                     "minItems":1
                  },
                  "references":{
                     "type":"array",
                     "required":false,
                     "items":[
                        {
                           "type":"object",
                           "required":false,
                           "additionalProperties":false,
                           "properties":{
                              "uid":{
                                 "type":"string",
                                 "required":true
                              },
                              "version":{
                                 "type":"string",
                                 "required":true,
                                 "pattern":"/^([\\(\\[\\)\\]].*[\\(\\[\\)\\]])|([\\d\\.]*)$/"
                              },
                              "type":{
                                 "type":"string",
                                 "required":true,
                                 "enum":[
                                    "depends",
                                    "recommends",
                                    "suggests",
                                    "conflicts",
                                    "provides"
                                 ]
                              }
                           }
                        }
                     ]
                  },
                  "forgeCompat":{
                     "type":"string",
                     "pattern":"/^([\\(\\[\\)\\]].*[\\(\\[\\)\\]])|([\\d\\.]*)$/"
                  },
                  "name":{
                     "type":"string",
                     "required":true
                  },
                  "url":{
                     "type":"string",
                     "required":true
                  },
                  "md5":{
                     "type":"string"
                  },
                  "downloadType":{
                     "type":"string",
                     "enum":[
                        "direct",
                        "parallel",
                        "sequential"
                     ]
                  },
                  "installType":{
                     "type":"string",
                     "required":true,
                     "enum":[
                        "forgeMod",
                        "forgeCoreMod",
                        "extract",
                        "configPack",
                        "group"
                     ]
                  },
                  "type":{
                     "type": "string"
                  }
               }
            }
         ]
      },
      "websiteUrl":{
         "type":"string",
         "required":false,
         "format":"uri"
      },
      "verifyUrl":{
         "type":"string",
         "required":false,
         "format":"uri"
      }
   }
}
