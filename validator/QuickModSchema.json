{
	"type":"object",
	"id": "http://jsonschema.net",
	"required":true,
	"schema":"http://json-schema.org/draft-04/schema#",
	"additionalProperties": false,
	"properties":{
		"authors": {
			"type": "object",
			"required": false
		},
		"categories": {
			"type":"array",
			"required":false,
			"uniqueItems": true,
			"items": {
				"type":"string"
			}
		},
		"references": {
			"type":"object",
			"required":false,
			"patternProperties": {
				"/^.*$/": {
					"type":"string",
					"format": "uri"
				}
			}
		},
		"description": {
			"type":"string",
			"required":false
		},
		"iconUrl": {
			"type":"string",
			"required":false,
			"format": "uri"
		},
		"logoUrl": {
			"type":"string",
			"required":false,
			"format": "uri"
		},
		"uid": {
			"type": "string",
			"required": true,
                        "minLength": 3
		},
		"modId": {
			"type":"string",
			"required":false
		},
		"name": {
			"type":"string",
			"required":true
		},
		"nemName": {
			"type":"string",
			"required":false
		},
		"tags": {
			"type":"array",
			"required":false,
			"items": {
				"type":"string"
			}
		},
		"updateUrl": {
			"type":"string",
			"required":true,
			"format": "uri"
		},
		"versionsUrl": {
			"type": "string",
			"required": true,
			"format": "uri"
		},
		"websiteUrl": {
			"type":"string",
			"required":false,
			"format": "uri"
		},
		"verifyUrl": {
			"type": "string",
			"required": false,
			"format": "uri"
		}
	}
}

